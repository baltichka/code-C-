#include <stdio.h>
#include <math.h>


int main()

{
    double a=5.1, b=1, c=-2, d;
    float x1, x2;
    
    d = pow(b,2) - 4*a*c;
    x1 = (-b + sqrt(d));
    x2 = (-b - sqrt(d));
    printf("d = %lf, x1 = %lf, x2 = %lf", d,x1,x2);

    return 0;
}
